<li style = "height: 30px">
	<p style="font-weight: bold;" >
	Name: {{name}},
	Rooms: {{rooms}},
	Check-In: {{checkin}},
	Check-Out: {{checkout}}
	{{#if status}}
	<button class="change-status btn btn-primary indigo darken-3" data-id="{{this.id}}" data-newStatus="{{this.status}}" style="height: 35px"> Check In<i class="material-icons right">work</i></button>
	{{/if}}
</p>
</li>
